# Server Configuration
PORT=5000
NODE_ENV=development

# Cache Settings
CACHE_TTL=3600000          # Cache time-to-live in ms (default: 1 hour)

# Scraping Configuration
SCRAPE_TIMEOUT=120000      # Max time per scraper in ms (default: 2 minutes)

# Rate Limiting
RATE_LIMIT_WINDOW=900000   # Rate limit window in ms (default: 15 minutes)
RATE_LIMIT_MAX=50          # Max requests per window (default: 50)
